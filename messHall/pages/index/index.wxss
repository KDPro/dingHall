@import"../../wxParse/wxParse.wxss";
page {
  height:100%;
}
.header {
  width:100%;
  height:50px;
  background-color: white;
  color:#505050;
  font-size:15px;
  text-align: center;
  line-height: 50px;
}
.other {
  height:calc(100% - 50px);
}
.img {
  width:100%;
  margin-bottom:15px;
}
.img image {
  width:100%;
}
.text {
  padding:0 15px;
  font-size:15px;
  color:#505050;
  text-align:justify; 
  line-height:1.6em;
}